#include<stdio.h>
#include<string.h>

int main()
{
    char str[256];
    char ch;
    printf("enter the string");
    scanf("%s",str);
    int i,n;
    n=strlen(str);
    int c;
    for(ch='a';ch<='z';ch++)
    {
         c=0;
        for(i=0;i<n;i++)
        {
                if(ch==str[i])
                {
                    c++;
                }
        }
/* if(c>0)
        {
        printf("%c  occurs %d \n",ch,c);
        }
        */
 if(c==1)
 {
     printf("%c only the non repeating alphabet",ch);
 }
    }


}
