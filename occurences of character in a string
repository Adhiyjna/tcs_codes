#include<stdio.h>
#include<string.h>

int main()
{int i;
    char tosearch;
    int count=0;
    char s[100];
    printf("enter the string");
    gets(s);

    printf("enter the search to be element");
    tosearch = getchar();
i=0;
    while(s[i]!='\0')
    {
        if(s[i]==tosearch)
        {
            count++;
        }
        i++;
    }
    printf("%c %d",tosearch,count);
}
